	"definitions": {
		"IPagedList[CredentialSet]": {
			"description": "The paged list result",
			"required": ["pageNumber",
			"pageSize",
			"totalItemCount",
			"totalPages",
			"hasPreviousPage",
			"hasNextPage",
			"items"],
			"type": "object",
			"properties": {
				"pageNumber": {
					"format": "int32",
					"description": "Gets the page number",
					"type": "integer",
					"readOnly": true
				},
				"pageSize": {
					"format": "int32",
					"description": "Gets the size of the page",
					"type": "integer",
					"readOnly": true
				},
				"totalItemCount": {
					"format": "int32",
					"description": "Gets the total item count",
					"type": "integer",
					"readOnly": true
				},
				"totalPages": {
					"format": "int32",
					"description": "Gets the total page count",
					"type": "integer",
					"readOnly": true
				},
				"hasPreviousPage": {
					"description": "Gets a value indicating whether this instance has previous page",
					"type": "boolean",
					"readOnly": true
				},
				"hasNextPage": {
					"description": "Gets a value indicating whether this instance has next page",
					"type": "boolean",
					"readOnly": true
				},
				"items": {
					"description": "Gets the items",
					"type": "array",
					"items": {
						"$ref": "#/definitions/CredentialSet"
					},
					"readOnly": true
				}
			}
		},
		"CredentialSet": {
			"description": "The Credential Set",
			"required": ["code",
			"name"],
			"type": "object",
			"properties": {
				"code": {
					"description": "Gets or sets the code of the Credential Set",
					"type": "string"
				},
				"name": {
					"description": "Gets or sets the name of the Credential Set",
					"type": "string"
				},
				"id": {
					"format": "int64",
					"description": "Gets or sets the identifier. The property value is missing/null in Private Credential Sets",
					"type": "integer"
				}
			}
		}
	},
